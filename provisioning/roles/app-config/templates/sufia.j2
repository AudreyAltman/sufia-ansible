<VirtualHost *:80>
ServerName vagrant-ubuntu-trusty-64
#www.yourhost.com
# !!! Be sure to point DocumentRoot to 'public'!
DocumentRoot /opt/sufia/public
  <Directory /opt/sufia/public>
        # This relaxes Apache security settings.
        AllowOverride all  
        # MultiViews must be turned off.  
        Options -MultiViews  
  </Directory>  
</VirtualHost>